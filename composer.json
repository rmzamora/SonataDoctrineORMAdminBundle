{
    "name": "rmzamora/doctrine-orm-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine ORM into the SonataAdminBundle, rmzamora forked",
    "keywords": ["Admin Generator", "admin", "sonata", "bootstrap", "generator", "rmzamora forked"],
    "homepage": "http://sonata-project.org/bundles/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "mell m. zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrineORMAdminBundle/contributors"
        }
    ],
    "require": {
        "doctrine/orm": "~2.3",
        "symfony/form": "~2.4",
        "symfony/security": "~2.4",
        "symfony/console": "~2.4",
        "symfony/doctrine-bridge": "~2.4",
        "symfony/symfony": "~2.4",
        "sonata-project/exporter": "~1.3,>=1.3.1",
        "rmzamora/admin-bundle": "~2.3",
         "sonata-project/core-bundle": "~2.2"
    },
    "suggest": {
        "simplethings/entity-audit-bundle": "If you want to support for versioning of entities and their associations."
    },
    "autoload": {
        "psr-0": { "Sonata\\DoctrineORMAdminBundle": "" }
    },
    "target-dir": "Sonata/DoctrineORMAdminBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
