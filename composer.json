    "require": {
        "doctrine/orm": ">=2.3,<2.4",
        "rmzamora/admin-bundle": "2.2.*@dev",
        "symfony/symfony": ">=2.2,<2.3",
        "knplabs/knp-menu-bundle": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Sonata\\DoctrineORMAdminBundle": "" }
